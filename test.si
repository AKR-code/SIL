@ Expression parsing stress test @

let a
let b
let c
let result

put "enter value for a: "
get a
put "enter value for b: "
get b
put "enter value for c: "
get c

@ precedence test @
result = a + b * c
put "a + b * c = ", result, "\n"

@ parentheses override @
result = (a + b) * c
put "(a + b) * c = ", result, "\n"

@ nested parentheses @
result = (a + (b * (c + 2))) / 2
put "(a + (b * (c + 2))) / 2 = ", result, "\n"

@ chained operations @
result = a + b + c * 2 - 4 / 2
put "a + b + c * 2 - 4 / 2 = ", result, "\n"
